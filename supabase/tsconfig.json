{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "lib": ["esnext", "dom"],
    "skipLibCheck": true,
    "noEmit": true,
    "baseUrl": ".",
    "outDir": "dist",
    "typeRoots": [
      "./node_modules/@types"
    ]
  },
  "include": ["./functions/**/*.ts"],
  "exclude": ["node_modules"]
}
